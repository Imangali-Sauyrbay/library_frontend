{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/i18n/locales/**/*.json"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@views/*": ["./src/views/*"]
    },
    "types": ["element-plus/global", "leaflet"]
  }
}
